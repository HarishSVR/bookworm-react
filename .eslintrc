{
    "extends":["airban","prettier","prettier-react"],
    "plugins":["prettier"],
    "parser":"babel-eslint",
    "parserOption":{
    "ecmaVersion":2016,
    "sourceType":"module",
    "ecmaFeatures":{
        "jsx":true
    }
},
"env":{
    "es6":true,
    "browser":true,
    "node":true
},
"rules":{
    "react/jsx-filename-extension":[1,{"extention"}]
}
}